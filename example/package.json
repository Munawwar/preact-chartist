{
  "name": "react-chartist",
  "scripts": {
    "browser-sync": "browser-sync start --server --files './bundle.js'",
    "build": "esbuild main.jsx --outfile=bundle.js --bundle --jsx-factory=h --jsx-fragment=Fragment",
    "build-gz": "esbuild main.jsx --outfile=bundle.min.js --bundle --minify --jsx-factory=h --jsx-fragment=Fragment; gzip bundle.min.js -fq"
  },
  "devDependencies": {
    "browser-sync": "^2.10.0",
    "esbuild": "^0.15.6",
    "typescript": "^4.8.2",
    "watchify": "^4.0.0"
  },
  "dependencies": {
    "chartist": "^1.1.2",
    "preact": "^8.5.3"
  }
}
